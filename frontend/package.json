{
  "name": "chatbot-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "build:debug": "node test-build.js",
    "build:production": "NODE_ENV=production npm run type-check && vite build --mode production",
    "build:prod": "NODE_ENV=production tsc && vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "preview": "vite preview --host 0.0.0.0 --port $PORT",
    "start": "npx serve -s dist -l $PORT -n",
    "dev": "vite --host 0.0.0.0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run --coverage --reporter=verbose --reporter=junit --outputFile=test-results.xml",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "serve": "npm run preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "axios": "^1.5.0",
    "zustand": "^4.4.1",
    "tailwindcss": "^3.3.0",
    "lucide-react": "^0.263.1",
    "react-hook-form": "^7.46.0",
    "react-hot-toast": "^2.4.1",
    "react-dropzone": "^14.2.3",
    "@tanstack/react-query": "^4.35.0",
    "@headlessui/react": "^1.7.17",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "serve": "^14.2.1",
    "express": "^4.18.2",
    "date-fns": "^2.30.0",
    "@sentry/react": "^7.77.0",
    "@sentry/tracing": "^7.77.0",
    "web-vitals": "^3.5.0",
    "workbox-window": "^7.0.0",
    "react-error-boundary": "^4.0.11",
    "react-helmet-async": "^1.3.0",
    "framer-motion": "^10.16.4",
    "recharts": "^2.8.0",

  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/ui": "^0.34.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.29",
    "prettier": "^3.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/user-event": "^14.4.3"
  }
}
