# Production Environment Configuration
VITE_APP_NAME=Enterprise AI Chatbot Platform
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production

# API Configuration for Railway
VITE_API_URL=https://your-backend-domain.railway.app
VITE_WS_URL=wss://your-backend-domain.railway.app
VITE_API_VERSION=v1
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_DEBUG_MODE=false

# Analytics & Monitoring
VITE_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
VITE_SENTRY_DSN=SENTRY_DSN_URL
VITE_HOTJAR_ID=HOTJAR_SITE_ID

# CDN & Assets
VITE_CDN_URL=https://cdn.chatbot.company.com
VITE_ASSETS_URL=https://assets.chatbot.company.com

# Security
VITE_CSP_NONCE=RANDOM_NONCE_VALUE
VITE_ENABLE_CSP=true

# Performance
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_PWA=true
VITE_CACHE_VERSION=1.0.0

# Branding
VITE_COMPANY_NAME=Your Company Name
VITE_SUPPORT_EMAIL=support@company.com
VITE_SUPPORT_URL=https://support.company.com
