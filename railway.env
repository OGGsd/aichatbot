# Railway Production Environment Variables

# Core Application
ENVIRONMENT=production
SECRET_KEY=railway-super-secret-key-2024-enterprise-grade
JWT_SECRET_KEY=railway-jwt-secret-key-2024-enterprise-grade

# Database (Railway PostgreSQL)
DATABASE_URL=postgresql://postgres:password@containers-us-west-xxx.railway.app:5432/railway

# Redis (Railway Redis)
REDIS_URL=redis://default:password@containers-us-west-xxx.railway.app:6379

# Admin Credentials
ADMIN_USERNAME=stefan@axiestudio.se
ADMIN_PASSWORD=STEfanjohn!12
ADMIN_EMAIL=stefan@axiestudio.se

# AI Providers
OPENAI_API_KEY=your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Performance & Monitoring
ENABLE_METRICS=true
PERFORMANCE_MONITORING_ENABLED=true
RATE_LIMIT_ENABLED=true
SECURITY_HEADERS_ENABLED=true

# Advanced Features
AI_SCALING_ENABLED=true
SECURITY_INTELLIGENCE_ENABLED=true
ADVANCED_ANALYTICS_ENABLED=true
CONVERSATION_INTELLIGENCE_ENABLED=true
CONTENT_MODERATION_ENABLED=true
KNOWLEDGE_GRAPH_ENABLED=true
REALTIME_COLLABORATION_ENABLED=true

# Production Settings
WORKERS=4
MAX_CONNECTIONS=1000
CORS_ORIGINS=https://*.railway.app,https://yourdomain.com

# Railway Specific
PORT=8000
HOST=0.0.0.0
